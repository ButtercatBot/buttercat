{
	"compilerOptions": {
		"incremental": true,
		"declaration": true,
		"inlineSourceMap": true,
		"esModuleInterop": true,
		"strict": true,
		"traceResolution": false,
		"listEmittedFiles": false,
		"listFiles": false,
		"pretty": true,
		"outDir": "./dist",
		"noEmit": true,
		"declarationMap": true,
		"sourceMap": true,
		"removeComments": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"useDefineForClassFields": true,
		"target": "ES2020",
		"module": "Node16",
		"moduleResolution": "nodenext",
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": true,
		"lib": [
			"es2017",
			"DOM",
			"DOM.Iterable"
		],
		"types": [
			"node"
		]
	},
	"compileOnSave": false
}
