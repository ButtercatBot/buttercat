{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [
		"**/.env.*local"
	],
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"env": [
				"NODE_ENV",
				"NEXT_PUBLIC_APP_URL"
			],
			"outputs": [
				"dist/**",
				".next/**",
				"!.next/cache/**"
			]
		},
		"test": {
			"dependsOn": [
				"^build"
			]
		},
		"lint": {
		}
	}
}
