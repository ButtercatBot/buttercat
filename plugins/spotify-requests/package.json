{
	"name": "@buttercatbot/spotify-requests",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/MarcDonald/buttercat",
		"directory": "plugins/spotify-requests"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"module": "dist/esm/index.js",
	"scripts": {
		"lint": "eslint \"**/*.ts*\"",
		"build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
		"build:esm": "tsc -p tsconfig.module.json",
		"build:cjs": "tsc -p tsconfig.json",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"start-standalone": "ts-node src/standalone.ts"
	},
	"dependencies": {
		"@buttercatbot/core": "*",
		"@buttercatbot/logger": "*",
		"express": "4.18.2",
		"fs": "0.0.1-security",
		"spotify-web-api-node": "5.0.2"
	},
	"devDependencies": {
		"@buttercatbot/eslint-config": "*",
		"@buttercatbot/tsconfig": "*",
		"@types/express": "4.17.17",
		"@types/node": "18.15.10",
		"@types/spotify-web-api-node": "5.0.7",
		"@types/tmi.js": "1.8.3",
		"@vitest/coverage-c8": "^0.29.8",
		"dotenv": "16.0.3",
		"eslint": "^7.32.0",
		"nodemon": "2.0.22",
		"prettier": "2.0.5",
		"ts-node": "10.9.1",
		"typescript": "5.0.4",
		"vite": "^4.3.1",
		"vitest": "^0.29.8",
		"zod": "^3.21.4"
	}
}
